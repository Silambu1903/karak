        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline3"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.3" />


        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline4"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.63" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline5"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.8" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline19"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.96" />

        <TextView
            android:id="@+id/textView_correction_factor"
            style="@style/textAppearence"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Flow rate"
            android:textSize="24sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/guideline8" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:src="@drawable/bg_dashboard_line"
            app:layout_constraintEnd_toEndOf="@+id/textView_correction_factor"
            app:layout_constraintStart_toStartOf="@+id/textView_correction_factor"
            app:layout_constraintTop_toBottomOf="@+id/textView_correction_factor" />

        <EditText
            android:id="@+id/flow_rate_karak"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_gravity="center"
            android:background="@drawable/edt_back_new"
            android:gravity="center"
            android:hint="0000"
            android:imeOptions="actionNext"
            android:inputType="number"
            android:maxLength="4"
            android:padding="5dp"
            android:textColor="@color/textColor"
            android:textColorHint="@color/ash"
            android:textSize="15sp"
            app:layout_constraintBottom_toTopOf="@+id/guideline9"
            app:layout_constraintEnd_toStartOf="@+id/flowrate_cardmom_karak"
            app:layout_constraintStart_toStartOf="@id/guideline1"
            app:layout_constraintTop_toTopOf="@+id/guideline12" />

        <EditText
            android:id="@+id/flowrate_cardmom_karak"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_gravity="center"
            android:layout_marginStart="15dp"
            android:background="@drawable/edt_back_new"
            android:gravity="center"
            android:hint="0000"
            android:imeOptions="actionNext"
            android:inputType="number"
            android:maxLength="4"
            android:padding="5dp"
            android:textColor="@color/textColor"
            android:textColorHint="@color/ash"
            android:textSize="15sp"
            app:layout_constraintBottom_toTopOf="@+id/guideline9"
            app:layout_constraintEnd_toEndOf="@id/guideline2"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/flow_rate_karak"
            app:layout_constraintTop_toTopOf="@+id/flow_rate_karak" />

        <TextView
            android:id="@+id/textView10"
            style="@style/textAppearence"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:gravity="center"
            android:text="@string/karak"
            app:layout_constraintEnd_toEndOf="@+id/flow_rate_karak"
            app:layout_constraintStart_toStartOf="@+id/flow_rate_karak"
            app:layout_constraintTop_toBottomOf="@+id/flow_rate_karak" />

        <TextView
            android:id="@+id/textView1"
            style="@style/textAppearence"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:gravity="center"
            android:text="@string/cardmom_karak"
