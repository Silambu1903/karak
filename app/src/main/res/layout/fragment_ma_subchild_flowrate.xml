            android:gravity="center"
            android:text="@string/milk"
            app:layout_constraintEnd_toEndOf="@+id/flow_rate_milk"
            app:layout_constraintStart_toStartOf="@+id/flow_rate_milk"
            app:layout_constraintTop_toBottomOf="@+id/flow_rate_milk" />

        <EditText
            android:id="@+id/flow_rate_sugar"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_gravity="center"
            android:background="@drawable/edt_back_new"
            android:gravity="center"
            android:hint="0000"
            android:imeOptions="actionNext"
            android:inputType="number"
            android:maxLength="4"
            android:padding="5dp"
            android:textColor="@color/textColor"
            android:textColorHint="@color/ash"
            android:textSize="15sp"
            app:layout_constraintBottom_toTopOf="@+id/guideline17"
            app:layout_constraintEnd_toEndOf="@+id/textView10"
            app:layout_constraintStart_toStartOf="@+id/textView10"
            app:layout_constraintTop_toTopOf="@+id/guideline16" />


        <EditText
            android:id="@+id/flow_rate_water"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_gravity="center"
            android:background="@drawable/edt_back_new"
            android:gravity="center"
            android:hint="0000"
            android:imeOptions="actionDone"
            android:inputType="number"
            android:maxLength="4"
            android:padding="5dp"
            android:textColor="@color/textColor"
            android:textColorHint="@color/ash"
            android:textSize="15sp"
            app:layout_constraintBottom_toBottomOf="@+id/guideline17"
            app:layout_constraintEnd_toEndOf="@+id/textView1"
            app:layout_constraintStart_toStartOf="@+id/textView1"
            app:layout_constraintTop_toTopOf="@+id/guideline16" />

        <TextView
            android:id="@+id/textView21"
            style="@style/textAppearence"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
