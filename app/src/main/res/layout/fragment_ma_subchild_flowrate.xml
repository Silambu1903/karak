            android:gravity="center"
            android:hint="0000"
            android:imeOptions="actionNext"
            android:inputType="number"
            android:maxLength="4"
            android:padding="5dp"
            android:textColor="@color/textColor"
            android:textColorHint="@color/ash"
            android:textSize="15sp"
            app:layout_constraintBottom_toBottomOf="@+id/flow_rate_masala_karak"
            app:layout_constraintEnd_toEndOf="@+id/textView1"
            app:layout_constraintStart_toStartOf="@+id/textView1"
            app:layout_constraintTop_toTopOf="@+id/guideline13" />

        <TextView
            android:id="@+id/textView11"
            style="@style/textAppearence"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:gravity="center"
            android:text="@string/masala_karak"
            app:layout_constraintEnd_toEndOf="@+id/flow_rate_masala_karak"
            app:layout_constraintStart_toStartOf="@+id/flow_rate_masala_karak"
            app:layout_constraintTop_toBottomOf="@+id/flow_rate_masala_karak" />

        <TextView
            android:id="@+id/textView12"
            style="@style/textAppearence"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:gravity="center"
            android:text="@string/coffee"
            app:layout_constraintEnd_toEndOf="@+id/flow_rate_coffee"
            app:layout_constraintStart_toStartOf="@+id/flow_rate_coffee"
            app:layout_constraintTop_toBottomOf="@+id/flow_rate_coffee" />

        <EditText
            android:id="@+id/flow_rate_ginger_karak"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_gravity="center"
            android:background="@drawable/edt_back_new"
            android:gravity="center"
            android:hint="0000"
            android:imeOptions="actionNext"
            android:inputType="number"
            android:maxLength="4"
            android:padding="5dp"
            android:textColor="@color/textColor"
            android:textColorHint="@color/ash"
            android:textSize="15sp"
            app:layout_constraintBottom_toTopOf="@+id/guideline14"
            app:layout_constraintEnd_toEndOf="@+id/textView11"
            app:layout_constraintStart_toStartOf="@+id/textView11"
            app:layout_constraintTop_toTopOf="@+id/guideline11" />


        <EditText
            android:id="@+id/flow_rate_milk"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_gravity="center"
            android:background="@drawable/edt_back_new"
            android:gravity="center"
            android:hint="0000"
            android:imeOptions="actionNext"
            android:inputType="number"
            android:maxLength="4"
            android:padding="5dp"
            android:textColor="@color/textColor"
            android:textColorHint="@color/ash"
            android:textSize="15sp"
            app:layout_constraintBottom_toTopOf="@+id/guideline14"
            app:layout_constraintEnd_toEndOf="@+id/textView12"
            app:layout_constraintStart_toStartOf="@+id/textView12"
            app:layout_constraintTop_toTopOf="@+id/guideline11" />

        <TextView
            android:id="@+id/textView13"
            style="@style/textAppearence"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:gravity="center"
            android:text="@string/ginger_karak"
            app:layout_constraintEnd_toEndOf="@+id/flow_rate_ginger_karak"
            app:layout_constraintStart_toStartOf="@+id/flow_rate_ginger_karak"
            app:layout_constraintTop_toBottomOf="@+id/flow_rate_ginger_karak" />

        <TextView
            android:id="@+id/textView14"
