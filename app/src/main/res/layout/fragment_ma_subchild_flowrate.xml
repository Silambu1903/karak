            app:layout_constraintEnd_toEndOf="@+id/textView10"
            app:layout_constraintStart_toStartOf="@+id/textView10"
            app:layout_constraintTop_toTopOf="@+id/guideline13" />


        <EditText
            android:id="@+id/flow_rate_coffee"
            android:layout_width="0dp"
